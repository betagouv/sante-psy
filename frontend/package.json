    "build:analyze": "node scripts/build.js --env.PRODANALYSER",
    "start": "webpack serve --config config/webpack.dev.hot.js --open \"Google Chrome\"",
    "dev": "webpack serve --config config/webpack.dev.hot.js --host 0.0.0.0",
    "lint": "node_modules/.bin/eslint --quiet . --ext .js,.jsx",
    "lint:fix": "node_modules/.bin/eslint --quiet --fix . --ext .js,.jsx",
    "test": "mocha --exit",
    "scalingo-prebuild": "npm clean-install --force --production=false && npm run build -- --env.PROD",
