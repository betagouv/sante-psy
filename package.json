{
  "name": "sante-psy",
  "version": "1.0.0",
  "engines": {
    "node": "^14.16.0",
    "npm": "^6.14.0"
  },
  "description": "Accompagnement Psychologique pour les Ã©tudiants",
  "main": "index.ts",
  "scripts": {
    "coverage": "nyc --reporter html --reporter text npm run test",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "tsc",
    "devKnex": "DEBUG=knex:query nodemon index.js",
    "ejslint": "ejslint **/*.ejs",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "seed": "knex seed:run",
    "listMigrations": "knex migrate:list",
    "migrate": "knex migrate:latest",
    "migrate-one": "knex migrate:up",
    "makeMigration": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "rollback-one": "knex migrate:down",
    "start": "node dist/index.js",
    "test": "ts-mocha --exit test/**",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "scalingo-prebuild": "npm --prefix frontend run scalingo-prebuild",
    "scalingo-postbuild": "tsc && cp -a frontend/dist dist/frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/sante-psy.git"
  },
  "author": "dev-transverse@beta.gouv.fr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betagouv/sante-psy/issues"
  },
  "homepage": "https://github.com/betagouv/sante-psy#readme",
  "dependencies": {
    "@gouvfr/dsfr": "^1.0.0-rc1.0",
    "@sentry/integrations": "^6.2.2",
    "@sentry/node": "^6.2.2",
    "@sentry/tracing": "^6.2.2",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-jwt": "^6.0.0",
    "express-rate-limit": "^5.2.6",
    "express-sanitizer": "^1.0.5",
    "express-session": "^1.17.1",
    "express-slow-down": "^1.4.0",
    "express-validator": "^6.10.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "helmet": "^4.4.1",
    "intl": "^1.2.5",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "lodash": "^4.17.20",
    "mobx": "^6.3.0",
    "nodemailer": "^6.4.18",
    "pg": "^8.5.1",
    "react-month-picker": "^2.2.1",
    "react-month-picker-input": "^1.3.8",
    "tabulator-tables": "^4.9.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "ejs-lint": "^1.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^5.0.9",
    "jwt-decode": "^3.1.2",
    "mocha": "^8.2.1",
    "nock": "^13.0.7",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pinst": "^2.1.4",
    "rewire": "^5.0.0",
    "sinon": "^9.2.4",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
