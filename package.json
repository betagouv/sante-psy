{
  "name": "sante-psy",
  "version": "1.0.0",
  "engines": {
    "node": "^14.16.0",
    "npm": "^6.14.0"
  },
  "description": "Accompagnement Psychologique pour les Ã©tudiants",
  "main": "index.ts",
  "scripts": {
    "coverage": "nyc --reporter html --reporter text npm run test",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "tsc",
    "devKnex": "DEBUG=knex:query nodemon index.js",
    "ejslint": "ejslint **/*.ejs",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "seed": "knex seed:run",
    "listMigrations": "knex migrate:list",
    "migrate": "knex migrate:latest",
    "migrate-one": "knex migrate:up",
    "makeMigration": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "rollback-one": "knex migrate:down",
    "start": "node dist/index.js",
    "test": "TEST_ENVIRONMENT=true ts-mocha --require @babel/register --exit test/**",
    "test:ci": "npm run test",
    "preinstall": "npx npm-force-resolutions",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "scalingo-prebuild": "npm --prefix frontend run scalingo-prebuild",
    "scalingo-postbuild": "npm --prefix frontend run scalingo-postbuild && npm run postbuild",
    "postbuild": "tsc && cp -a frontend/dist dist/frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/sante-psy.git"
  },
  "author": "dev-transverse@beta.gouv.fr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betagouv/sante-psy/issues"
  },
  "homepage": "https://github.com/betagouv/sante-psy#readme",
  "dependencies": {
    "@sentry/integrations": "^6.9.0",
    "@sentry/node": "^6.9.0",
    "@sentry/tracing": "^6.9.0",
    "@types/node": "^16.3.2",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-rate-limit": "^5.3.0",
    "express-sanitizer": "^1.0.6",
    "express-session": "^1.17.2",
    "express-slow-down": "^1.4.0",
    "express-validator": "^6.12.0",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "helmet": "^4.6.0",
    "intl": "^1.2.5",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "mime-types": "^2.1.31",
    "node-crisp-api": "^1.12.2",
    "nodemailer": "^6.6.3",
    "pg": "^8.6.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@babel/register": "^7.14.5",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/mocha": "^8.2.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-core": "^6.26.3",
    "babel-plugin-rewire": "^1.2.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "ejs-lint": "^1.2.1",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "jwt-decode": "^3.1.2",
    "mocha": "^9.0.2",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "npm-force-resolutions": "0.0.10",
    "nyc": "^15.1.0",
    "pinst": "^2.1.6",
    "rewire": "^5.0.0",
    "sinon": "^11.1.1",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.8"
  },
  "resolutions": {
    "socket.io-parser": "3.4.1",
    "xmlhttprequest-ssl": "1.6.2"
  }
}
