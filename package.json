{
  "name": "sante-psy",
  "version": "1.0.0",
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "description": "Accompagnement Psychologique pour les Ã©tudiants",
  "main": "index.ts",
  "scripts": {
    "coverage": "nyc --reporter html --reporter text npm run test",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "tsc",
    "devKnex": "DEBUG=knex:query nodemon index.js",
    "ejslint": "ejslint **/*.ejs",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "seed": "knex seed:run",
    "listMigrations": "knex migrate:list",
    "migrate": "knex migrate:latest",
    "migrate-one": "knex migrate:up",
    "makeMigration": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "rollback-one": "knex migrate:down",
    "start": "node dist/index.js",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "set TEST_ENVIRONMENT=true && ts-mocha --require @babel/register --exit test/** test/**/*",
    "test:frontend": "npm --prefix frontend run test",
    "test:ci": "wait-on --timeout 60000 https://api-annuaire-sante.herokuapp.com/annuaire/annuaire.json?Identification+nationale+PP__in=0 && npm run test",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "scalingo-prebuild": "npm --prefix frontend run scalingo-prebuild",
    "scalingo-postbuild": "npm --prefix frontend run scalingo-postbuild && npm run postbuild",
    "postbuild": "tsc && cp -a frontend/dist dist/frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/sante-psy.git"
  },
  "author": "dev-transverse@beta.gouv.fr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betagouv/sante-psy/issues"
  },
  "homepage": "https://github.com/betagouv/sante-psy#readme",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@sentry/integrations": "^7.33.0",
    "@sentry/node": "^7.33.0",
    "@sentry/tracing": "^7.33.0",
    "@sentry/types": "^7.33.0",
    "@types/chai": "^4.3.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crisp-api": "^5.1.0",
    "cron": "^2.2.0",
    "crypto": "^1.0.1",
    "dompurify": "^2.4.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.19.2",
    "express-jwt": "^8.3.0",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "express-slow-down": "^1.5.0",
    "express-validator": "^6.14.3",
    "graphql": "^16.8.1",
    "graphql-request": "^5.1.0",
    "helmet": "^6.0.1",
    "intl": "^1.2.5",
    "jsdom": "^21.1.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.9",
    "pg": "^8.8.0",
    "string-similarity": "^4.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@babel/register": "^7.18.9",
    "@types/express": "^4.17.16",
    "@types/express-jwt": "^6.0.4",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-core": "^6.26.3",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "ejs-lint": "^2.0.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "jwt-decode": "^3.1.2",
    "mocha": "^10.2.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "pinst": "^3.0.0",
    "sinon": "^15.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0"
  }
}
